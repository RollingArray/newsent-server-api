{
    "require": {
        "php-di/php-di": "^6.3",
        "psr/container": "^1.0",
        "selective/basepath": "^2.0",
        "slim/psr7": "^1.4",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
